<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/dataTable"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="3dp"
    android:paddingTop="1dp"
    android:stretchColumns="0,1,2,3,4">

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingTop="1dp"
        android:paddingBottom="1dp"
        android:visibility="visible">

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="1dp"
            android:background="#F3CBAC"
            android:padding="5dp"
            android:text="现存确诊"
            android:textAlignment="center" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="1dp"
            android:background="#E6D4A8"
            android:padding="5dp"
            android:text="累计确诊"
            android:textAlignment="center" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="1dp"
            android:background="#27CBE49A"
            android:padding="5dp"
            android:text="累计治愈"
            android:textAlignment="center" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="1dp"
            android:background="#EDC0CB"
            android:padding="5dp"
            android:text="累计死亡"
            android:textAlignment="center" />
    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingTop="1dp"
        android:paddingBottom="1dp">

        <TextView
            android:id="@+id/textCurrentSick"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="1dp"
            android:background="#EAEFDD"
            android:padding="5dp"
            android:text="TextView"
            android:textAlignment="center" />

        <TextView
            android:id="@+id/textTotalSick"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="1dp"
            android:padding="5dp"
            android:background="#EAEFDD"
            android:text="TextView"
            android:textAlignment="center" />

        <TextView
            android:id="@+id/textTotalDeath"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="1dp"
            android:padding="5dp"
            android:background="#EAEFDD"
            android:text="TextView"
            android:textAlignment="center" />

        <TextView
            android:id="@+id/textTotalCured"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="1dp"
            android:background="#EAEFDD"
            android:padding="5dp"
            android:text="TextView"
            android:textAlignment="center" />
    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingTop="1dp"
        android:paddingBottom="1dp">

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="1dp"
            android:background="#EDE2C6"
            android:padding="5dp"
            android:text="较昨日"
            android:textAlignment="center" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="1dp"
            android:background="#EDE2C6"
            android:padding="5dp"
            android:text="较昨日"
            android:textAlignment="center" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="1dp"
            android:background="#EDE2C6"
            android:padding="5dp"
            android:text="较昨日"
            android:textAlignment="center" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="1dp"
            android:background="#EDE2C6"
            android:padding="5dp"
            android:text="较昨日"
            android:textAlignment="center" />
    </TableRow>

    <TableRow
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingTop="1dp"
        android:paddingBottom="1dp">

        <TextView
            android:id="@+id/textNewCurSick"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#EAEFDD"
            android:layout_marginLeft="1dp"
            android:padding="5dp"
            android:text="TextView"
            android:textAlignment="center" />

        <TextView
            android:id="@+id/textNewTotalSick"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="1dp"
            android:background="#EAEFDD"
            android:padding="5dp"
            android:text="TextView"
            android:textAlignment="center" />

        <TextView
            android:id="@+id/textNewTotalDeath"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#EAEFDD"
            android:layout_marginLeft="1dp"
            android:padding="5dp"
            android:text="TextView"
            android:textAlignment="center" />

        <TextView
            android:id="@+id/textNewTotalCured"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#EAEFDD"
            android:layout_marginLeft="1dp"
            android:padding="5dp"
            android:text="TextView"
            android:textAlignment="center" />
    </TableRow>

</TableLayout>